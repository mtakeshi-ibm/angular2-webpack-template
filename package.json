{
  "name": "angular2-webpack-template",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm-run-all clean:dist webpack -- --config webpack-config/webpack.config.dev.js --bail --progress --profile",
    "build:prod": "npm-run-all clean:dist webpack -- --config webpack-config/webpack.config.prod.js --bail --progress --profile",
    "build": "npm run build:dev",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm-run-all cache-clean rimraf ",
    "cache-clean": "npm cache clean",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "typedoc": "typedoc",
    "rimraf": "rimraf",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "mocha --require should --reporter xunit-file",
    "test-coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "test-e2e": "protractor ./test-e2e/chrome.js",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint",
    "nsp": "nsp check",
    "start": "npm run server:dev",
    "server:dev": "node ./index",
    "start-dev-supervisor": "supervisor -s -t -i node_modules,package.json,src/app,upload -e node,js,json ./index",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "typings": "typings init"
  },
  "engines": {
    "node": ">=4.3.1",
    "npm": ">= 3"
  },
  "keywords": [
  ],
  "author": "IBM Japan, Ltd.",
  "license": "MIT",
