    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "@angularclass/hmr": "1.2.1",
    "basic-auth-connect": "1.0.0",
    "body-parser": "1.15.2",
    "cfenv": "1.0.3",
    "compression": "1.6.2",
    "config": "1.21.0",
    "cookie-parser": "1.4.3",
    "core-js": "2.4.1",
    "cors": "2.8.1",
    "dotenv": "2.0.0",
    "express": "4.14.0",
    "i18n": "0.8.3",
    "jquery": "3.1.1",
    "log4js": "0.6.38",
